<div class="container">
    <h1> TEST</h1>
    <ul>
        <% @tweets.each do |tweet| %>
            <li>
                <%= tweet.username %>: <%= tweet.status %>
            </li>
        <% end %> 
        
        <form method="post" action="/tweets">
          <p>Username: </p><input type="text" name="username">
          <p>Status: </p><input type="text" name="status">
          <input type="submit">
        </form>
    </ul>
</div>